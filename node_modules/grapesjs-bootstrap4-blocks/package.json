{
  "name": "grapesjs-bootstrap4-blocks",
  "version": "0.2.16",
  "description": "GrapesJS Bootstrap v4 Blocks Plugin",
  "main": "dist/grapesjs-bootstrap4-blocks.min.js",
  "scripts": {
    "lint": "eslint src",
    "v:patch": "npm version --no-git-tag-version patch",
    "v:minor": "npm version --no-git-tag-version minor",
    "build": "npm run v:patch && webpack --env production",
    "build-dev": "npm run && webpack --env development",
    "start": "webpack-cli serve --env development --progress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jcsofts/grapesjs-blocks-bootstrap4.git"
  },
  "keywords": [
    "grapesjs",
    "plugin",
    "bootstrap"
  ],
  "author": "fedora",
  "license": "BSD-3-Clause",
  "peerDependencies": {
    "grapesjs": "^0.16.34 || ^0.17.4"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/preset-env": "^7.11.5",
    "@webpack-cli/init": "^1.0.3",
    "babel-loader": "^8.1.0",
    "eslint": "^7.8.1",
    "html-webpack-plugin": "^4.4.1",
    "raw-loader": "^4.0.1",
    "terser-webpack-plugin": "^5.0.3",
    "webpack": "^5.6.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "bugs": {
    "url": "https://github.com/jcsofts/grapesjs-blocks-bootstrap4/issues"
  },
  "homepage": "https://github.com/jcsofts/grapesjs-blocks-bootstrap4#readme",
  "dependencies": {
    "npm": "^7.0.14"
  }
}
